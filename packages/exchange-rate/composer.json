{
  "name": "alhaji-aki/exchange-rate",
  "description": "exchange rate package",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "AlhajiAki\\ExchangeRate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlhajiAki\\ExchangeRate\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Alhaji Aki",
      "email": "abdulkudus2922@gmail.com"
    }
  ],
  "scripts": {
    "analyse": "./vendor/bin/phpstan",
    "test": "./vendor/bin/phpunit",
    "format": "./vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.2",
    "guzzlehttp/guzzle": "^7.2",
    "illuminate/http": "^10.0",
    "illuminate/collections": "^10.0",
    "illuminate/support": "^10.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "mockery/mockery": "^1.3",
    "nunomaduro/larastan": "^2.0",
    "nunomaduro/collision": "^7.0",
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.1"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "AlhajiAki\\ExchangeRate\\ExchangeRateServiceProvider"
      ]
    }
  }
}
